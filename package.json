{
  "name": "redux-api-call",
  "version": "1.1.0",
  "license": "MIT",
  "description": "Redux utilities for making API calls",
  "repository": "https://github.com/tungv/redux-api-call.git",
  "scripts": {
    "prepublish": "npm run build",
    "postpublish": "npm run build:clean",
    "build:clean": "rm -rf dist || true",
    "prebuild": "npm run build:clean",
    "build": "rollup -c",
    "test": "jest --coverage --forceExit",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js)$",
    "setupFiles": ["isomorphic-fetch/fetch-npm-node.js"],
    "testPathIgnorePatterns": ["/node_modules/", "vendor", ".build"],
    "moduleFileExtensions": ["js", "json", "node"],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy",
      "^.+\\.(scss|css|less)(\\?\\w*)?$": "identity-obj-proxy"
    },
    "coveragePathIgnorePatterns": ["/node_modules/", "__mocks__", "__tests__"]
  },
  "files": ["dist"],
  "main": "dist/redux-api-call.cjs.js",
  "module": "dist/redux-api-call.es.js",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-jest": "^20.0.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-stage-1": "^6.13.0",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "fetch-mock": "^5.5.0",
    "isomorphic-fetch": "^2.2.1",
    "jest-cli": "^20.0.1",
    "lodash": "^4.16.1",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.0",
    "rollup": "^0.45.2",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-cleanup": "^0.1.4",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-strip": "^1.1.1",
    "rollup-plugin-stub": "^1.1.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rxjs": "^5.4.2",
    "sinon": "^1.17.6",
    "source-map-support": "^0.4.2",
    "timekeeper": "^0.1.1"
  },
  "dependencies": {
    "redux-api-call-adapter-dedupe": "https://github.com/toan2406/redux-api-call-adapter-dedupe#v1.0.0",
    "redux-api-call-adapter-fetch": "https://github.com/toan2406/redux-api-call-adapter-fetch#v1.1.0",
    "redux-api-call-adapter-json": "https://github.com/toan2406/redux-api-call-adapter-json#v1.0.0"
  },
  "peerDependencies": {
    "redux": "^3.6.0 || ^4.0.0"
  }
}
